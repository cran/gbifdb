citHeader("To cite gbifdb in publications use:")

citEntry(
  entry    = "misc",
  title    = paste("Global Biodiversity Information Facility (GBIF)
                   Species Occurrences"),
  year     = "2021",
  url      = "https://registry.opendata.aws/gbif",
  textVersion = paste("Global Biodiversity Information Facility (GBIF) Species",
  "Occurrences was accessed on", Sys.Date(), 
  "from https://registry.opendata.aws/gbif.")

  )

